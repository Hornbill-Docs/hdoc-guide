{
    "docId": "hdoc-guide",
    "pdfGeneration": {
        "enable": true,
        "exclude_paths": []
    },
    "validation": {
        "exclude_links": [
            "/style-guide/",
            "http://127.0.0.1:3000/",
            "mailto:docs@hornbill.com"
        ],
        "exclude_h1_count": [
            "hdoc-guide/hdocbook/markdown"
        ],
        "exclude_spellcheck": [
            {
                "document_path": "hdoc-guide/hdocbook/specification.md",
                "words": [
                    "labelled",
                    "favourite"
                ]
            }
        ]
    }
}
